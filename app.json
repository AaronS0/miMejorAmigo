{
  "expo": {
    "name": "Mi mejor amigo",
    "slug": "mi-mejor-amigo",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "mimejoramigo",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#D9A05B",
        "foregroundImage": "./assets/images/splash.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.mimejoramigo",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#D9A05B"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#D9A05B",
          "dark": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#D9A05B"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "22b3f397-9187-40fa-9733-85d2edfa5dff"
      }
    },
    "owner": "aarons0"
  }
}
